generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  avatar       String
  bibeIds      String[]
  email        String
  firstName    String
  followerIds  String[]
  followingIds String[]
  lastName     String
  password     String
  username     String
}
